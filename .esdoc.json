{
  "destination": "./docs",
  "plugins": [{
  	"name":"esdoc-node"
  },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "excludes": [
            "\\.config\\.js$"
          ],
          "includes": [
            "(spec|Spec|test|Test)\\.js$"
          ],
          "interfaces": [
            "describe",
            "it",
            "context",
            "suite",
            "test"
          ],
          "source": "./test/"
        }
      }
    }
  ],
  "source": "./src"
}